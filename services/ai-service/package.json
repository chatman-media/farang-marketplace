    "@marketplace/database-schema": "workspace:*",
    "@marketplace/logger": "workspace:*",
    "axios": "^1.12.1",
    "bcryptjs": "3.0.2",
    "compromise": "^14.14.4",
    "dotenv": "17.2.2",
    "drizzle-orm": "^0.44.5",
    "fastify": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "ml-matrix": "^6.12.1",
    "multer": "2.0.2",
    "natural": "8.1.0",
    "node-cron": "4.2.1",
