{
  "name": "ai-service",
  "version": "1.0.0",
  "description": "AI recommendation engine and machine learning service for Thailand Marketplace",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "3.0.2",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "dotenv": "17.2.2",
    "drizzle-orm": "^0.44.5",
    "express": "5.1.0",
    "express-rate-limit": "8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "8.1.0",
    "jsonwebtoken": "^9.0.2",
    "ml-matrix": "^6.12.1",
    "multer": "2.0.2",
    "natural": "8.1.0",
    "node-cron": "4.2.1",
    "openai": "5.19.1",
    "postgres": "^3.4.7",
    "redis": "5.8.2",
    "sentiment": "^5.0.2",
    "sharp": "^0.34.3",
    "stopword": "^3.1.5",
    "uuid": "12.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "2.0.0",
    "@types/natural": "6.0.1",
    "@types/node": "24.3.1",
    "@types/node-cron": "^3.0.11",
    "@types/sentiment": "^5.0.4",
    "@types/stopword": "^2.0.3",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.42.0",
    "@typescript-eslint/parser": "8.42.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "9.35.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "3.2.4"
  },
  "keywords": [
    "ai",
    "machine-learning",
    "recommendation-engine",
    "nlp",
    "marketplace",
    "thailand",
    "typescript",
    "express"
