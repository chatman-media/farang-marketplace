{
  "name": "@thailand-marketplace/agency-service",
  "version": "1.0.0",
  "description": "Agency management service for Thailand Marketplace platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "agency",
    "service",
    "marketplace",
    "thailand",
    "commission",
    "management"
  ],
  "author": "Thailand Marketplace Team",
  "license": "MIT",
  "dependencies": {

    "express": "5.1.0",
    "cors": "^2.8.5",
    "helmet": "8.1.0",
    "express-rate-limit": "8.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.36.4",
    "postgres": "^3.4.7",
    "uuid": "^11.0.3",
    "dotenv": "^16.4.7",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@types/express": "5.0.3",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^10.0.0",
    "@types/multer": "^1.4.12",
    "drizzle-kit": "^0.28.1",
    "typescript": "^5.9.2",
    "vitest": "3.2.4",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.0.0"
  }
}
