{
  "name": "@thailand-marketplace/agency-service",
  "version": "1.0.0",
  "description": "Agency management service for Thailand Marketplace platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "agency",
    "service",
    "marketplace",
    "thailand",
    "commission",
    "management"
  ],
  "author": "Thailand Marketplace Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.3.0",
    "bcryptjs": "3.0.2",
    "dotenv": "17.2.2",
    "drizzle-orm": "^0.44.5",
    "fastify": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "2.0.2",
    "postgres": "^3.4.7",
    "sharp": "^0.34.3",
    "uuid": "13.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/bcryptjs": "3.0.0",
    "@types/uuid": "^10.0.0",
    "@types/multer": "2.0.0",
    "drizzle-kit": "^0.31.4",
    "typescript": "^5.9.2",
    "vitest": "3.2.4",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4"
  }
}
