{
  "name": "@marketplace/booking-service",
  "version": "1.0.0",
  "description": "Booking and transaction management service for Thailand Marketplace",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@marketplace/shared-types": "workspace:*",
    "@marketplace/database-schema": "workspace:*",
    "@marketplace/logger": "workspace:*",
    "bcryptjs": "3.0.2",
    "dotenv": "17.2.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "fastify": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "4.2.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "redis": "5.8.2",
    "uuid": "13.0.0",
    "winston": "^3.17.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.3.1",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.35.0",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "vitest": "3.2.4"
  },
  "keywords": [
    "marketplace",
    "booking",
    "thailand",
    "rental",
    "transactions",
    "payments"
  ],
  "author": "Thailand Marketplace Team",
  "license": "MIT"
}
